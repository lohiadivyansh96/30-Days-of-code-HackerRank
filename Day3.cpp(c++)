int main(){
    int N;
    cin >> N;
    
    if(N%2 != 0) //if N IS ODD
    {
        cout<<"Weird" ;   
    }
    
    if(N%2==0 && N>1 && N<6) //if N IS Even
    {
        cout<<"Not Weird"  ;  
    }
    
        if(N%2==0 && N>5 && N<21) //if N IS Even
    {
        cout<<"Weird"    ;
    }
    
    if(N%2==0 && N>20) //if N IS ODD
    {
        cout<<"Not Weird"    ;
    }
    return 0;
}
